<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jain.tirthankarvani">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/tv_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/tv_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".VTCalendar" />
        <activity android:name=".DisplayListData" />
        <activity android:name=".NavkarMantra" />
        <activity android:name=".DisplayMessageActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".VratNotification">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>